{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd-mobile"
      }
    ],
    "transform-decorators-legacy",
    "transform-async-to-generator",
    "transform-class-properties",
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./components",
          "@constants": "./constants",
          "@stores": "./stores",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
